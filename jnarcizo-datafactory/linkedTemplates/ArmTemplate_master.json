{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "jnarcizo-datafactory"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks1\""
		},
		"AzureDatabricks_DataFactory_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_DataFactory\""
		},
		"Azure_CapaOro_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"Azure_CapaOro\""
		},
		"CapaPlata_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"CapaPlata\""
		},
		"Capa_Oro_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"Capa_Oro\""
		},
		"LINK_ADLS_DATAAFACTORY_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"LINK_ADLS_DATAAFACTORY\""
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0714-185528-r202hbdu"
		},
		"AzureDatabricks_DataFactory_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0714-185528-r202hbdu"
		},
		"Azure_CapaOro_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0714-185528-r202hbdu"
		},
		"CapaPlata_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0714-185528-r202hbdu"
		},
		"Capa_Oro_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0714-185528-r202hbdu"
		},
		"LINK_ADLS_DATAAFACTORY_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://narcizo123.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "jnarcizo-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricks_DataFactory_accessToken": {
						"value": "[parameters('AzureDatabricks_DataFactory_accessToken')]"
					},
					"Azure_CapaOro_accessToken": {
						"value": "[parameters('Azure_CapaOro_accessToken')]"
					},
					"CapaPlata_accessToken": {
						"value": "[parameters('CapaPlata_accessToken')]"
					},
					"Capa_Oro_accessToken": {
						"value": "[parameters('Capa_Oro_accessToken')]"
					},
					"LINK_ADLS_DATAAFACTORY_accountKey": {
						"value": "[parameters('LINK_ADLS_DATAAFACTORY_accountKey')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks_DataFactory_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_DataFactory_properties_typeProperties_existingClusterId')]"
					},
					"Azure_CapaOro_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Azure_CapaOro_properties_typeProperties_existingClusterId')]"
					},
					"CapaPlata_properties_typeProperties_existingClusterId": {
						"value": "[parameters('CapaPlata_properties_typeProperties_existingClusterId')]"
					},
					"Capa_Oro_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Capa_Oro_properties_typeProperties_existingClusterId')]"
					},
					"LINK_ADLS_DATAAFACTORY_properties_typeProperties_url": {
						"value": "[parameters('LINK_ADLS_DATAAFACTORY_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}